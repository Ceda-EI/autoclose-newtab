{
	"manifest_version": 2,
	"name": "Auto Close New Tab",
	"version": "1.0",

	"description": "Automatically closes the tab whenever you open a new tab.",
	"homepage_url": "https://gitlab.com/ceda_ei/autoclose-newtab",

	"icons": {
		"48": "icons/icon.svg",
		"96": "icons/icon.svg"
	},

	"permissions": [
		"tabs"
	],

	"background": {
		"scripts": ["background.js"]
	}
}
